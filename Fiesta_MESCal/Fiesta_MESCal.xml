<WaNoTemplate>
  <WaNoRoot name="Fiesta_MESCal">
    <WaNoFile logical_filename="sphere_mm.pdb" name="MM Structure">MM pdb file</WaNoFile>
    <WaNoGroup name="Microelectrostatic Input Files">
      <WaNoMultipleOf name="Molecular species">
	<Element id="0">
	  <WaNoString name="Molecule type">name (3chars)</WaNoString>
	  <WaNoFile logical_filename="mei_{{ path[2] }}.mei" name="MEI file">mei file</WaNoFile>
	</Element>
      </WaNoMultipleOf>
    </WaNoGroup>
    <WaNoGroup name="Options">
      <WaNoDictBox name="SCF control">
	<WaNoFloat name="Energy tolerance (eV)">1e-6</WaNoFloat>
	<WaNoInt name="Max. number of iterations">1000</WaNoInt>
	<WaNoFloat name="Damping">0.7</WaNoFloat>
	<WaNoFloat name="Max. energy (eV/molecule)">1.0</WaNoFloat>
      </WaNoDictBox>
    </WaNoGroup>
  </WaNoRoot>
  <WaNoExecCommand>bash ./exec.script
  </WaNoExecCommand>
 <WaNoInputFiles>
   <WaNoInputFile logical_filename="mescal.inp">inputtemplate.inp</WaNoInputFile> 
   <WaNoInputFile logical_filename="exec.script">exec_template.dat</WaNoInputFile>
   <WaNoInputFile logical_filename="Fiesta.in">Fiesta_template.dat</WaNoInputFile>   
 </WaNoInputFiles>
 <WaNoOutputFiles>
    <WaNoOutputFile>log_f2m</WaNoOutputFile>
    <WaNoOutputFile>Vc_creac.mat</WaNoOutputFile>
 </WaNoOutputFiles>
</WaNoTemplate>




