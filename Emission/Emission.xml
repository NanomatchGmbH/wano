 </WaNoRoot>
<WaNoExecCommand>export NANOVER="V3"; source $NANOMATCH/$NANOVER/configs/quantumpatch.config; python3 emission_run.py</WaNoExecCommand>
<WaNoInputFiles>
    <WaNoInputFile logical_filename="emission_run.py">emission_run.py</WaNoInputFile>
</WaNoInputFiles>
<WaNoOutputFiles>
     <WaNoOutputFile>report.zip</WaNoOutputFile>
     <WaNoOutputFile>orientation_analysis_input.yml</WaNoOutputFile>
</WaNoOutputFiles>
</WaNoTemplate>
