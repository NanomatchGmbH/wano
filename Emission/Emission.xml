            </WaNoDropDown>
        </WaNoDictBox>
        <WaNoDropDown name="Emission Type">
            <Entry id="0" chosen="True">Fluorescence</Entry>
            <Entry id="1">Phosphorescence</Entry>
        </WaNoDropDown>
        <WaNoDictBox visibility_condition="'%s' == 'Fluorescence'" visibility_var_path="Emission Type" name="Fluorescence Calculation Options">
            <WaNoDropDown name="Basis">
                <Entry id="0">STO-3G</Entry>
                <Entry id="1">4-31g hondo</Entry>
