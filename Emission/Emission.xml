            </WaNoDropDown>
            <WaNoDropDown name="Functional">
                <Entry id="0" chosen="True">b3-lyp</Entry>
            </WaNoDropDown>
            <WaNoInt name="Excitations">3</WaNoInt>
        </WaNoDictBox>
        <WaNoBool name="Automatic Center Atom">True</WaNoBool>
        <WaNoInt visibility_condition="%s == False" visibility_var_path="Automatic Center Atom" name="Center Atom">0</WaNoInt>
        <WaNoBool name="Orientation Analysis Output">False</WaNoBool>
        <WaNoDictBox visibility_condition="%s == True" visibility_var_path="Orientation Analysis Output" name="Internal Atomic Basis">
