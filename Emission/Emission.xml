            <WaNoDropDown name="Functional">
                <Entry id="0">SVWN</Entry>
                <Entry id="1">SVWN_Gaussian</Entry>
                <Entry id="2">PWLDA</Entry>
                <Entry id="3">BLYP</Entry>
                <Entry id="4">BVWN</Entry>
