<WaNoTemplate>
 <WaNoRoot name="DihedralParametrizer">
  <WaNoFile logical_filename="molecule.pdb" name="Deposit PDB">PDB input file</WaNoFile>
  <WaNoFile logical_filename="molecule.spf" name="Deposit SPF">SPF input file</WaNoFile>
  <WaNoDictBox name="Parameters">
      <WaNoChoice name="Basis">
        <Entry id="0" chosen="True" >SV(P)</Entry>
        <Entry id="1">def2-SV(P)</Entry>
        <Entry id="2">SVP</Entry>
        <Entry id="3">def2-SVP</Entry>
        <Entry id="4">TZVP</Entry>
        <Entry id="5">def2-TZVP</Entry>
        <Entry id="6">TZVPP</Entry>
        <Entry id="7">def2-TZVPP</Entry>
      </WaNoChoice>
     <WaNoChoice name="Functional">
        <Entry id="0" chosen="True" >b-p</Entry>
        <Entry id="1">b3-lyp</Entry>
      </WaNoChoice>
     <WaNoInt name="Number of Steps">40</WaNoInt>
  </WaNoDictBox>
  <WaNoDictBox name="Hardware Parameters">
    <WaNoChoice name="Engine">
    <Entry id="0" chosen="True" >Turbomole</Entry>
    <Entry id="1">Gaussian</Entry>
    </WaNoChoice>
    <WaNoString name="GaussianSCFHeader"></WaNoString>
    <WaNoFloat name="DFT Memory [MB]">1000</WaNoFloat>
  </WaNoDictBox>
 </WaNoRoot>
 <WaNoExecCommand>bash ./dhparam_intro.sh</WaNoExecCommand>
 <WaNoInputFiles>
   <WaNoInputFile logical_filename="dhparam_intro.sh">dhparam_intro.sh</WaNoInputFile>
 </WaNoInputFiles>
 <WaNoOutputFiles>
    <WaNoOutputFile>molecule_dh.pdb</WaNoOutputFile>
    <WaNoOutputFile>molecule_dh.spf</WaNoOutputFile>
    <WaNoOutputFile>*.png</WaNoOutputFile>    
 </WaNoOutputFiles>
</WaNoTemplate>
