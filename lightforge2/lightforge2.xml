          </WaNoDictBox>
          <WaNoDictBox name="x_axis">
            <WaNoBool name="Temperature">False</WaNoBool>
            <WaNoBool name="field_strength">False</WaNoBool>
          </WaNoDictBox>
        </WaNoDictBox>
      </WaNoGroup>

  </WaNoTabs>
