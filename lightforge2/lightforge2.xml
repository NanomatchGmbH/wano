          <WaNoBool name="get_hole_transport_level">False</WaNoBool>
        </WaNoDictBox>
        <WaNoBool name="analyse_ion_dop">False</WaNoBool>
        <WaNoDictBox name="current_analysis">
          <WaNoDictBox name="y_axis">
            <WaNoBool name="mobility">False</WaNoBool>
