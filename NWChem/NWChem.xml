<WaNoTemplate>
 <WaNoRoot name="NWChem">

  <WaNoFile logical_filename="molecule.xyz" name="Molecule">Put .xyz file here</WaNoFile>

  <WaNoDictBox name="Geometry optimization">
     <WaNoBool name="enabled">false</WaNoBool>
     <WaNoInt name="Number of steps">100</WaNoInt>
  </WaNoDictBox>

  <WaNoDictBox name="Simulation parameters">

    <WaNoChoice name="Functional">
    <Entry id="0" chosen="True" >becke88 perdew86</Entry>
    <Entry id="1">b3lyp</Entry>
    </WaNoChoice>

    <WaNoChoice name="Basis-set">
    <Entry id="0">Def2-SVP</Entry>
    <Entry id="1">Def2-TZVP</Entry>
    <Entry id="2" chosen="True" >3-21g</Entry>
    <Entry id="3">6-31g</Entry>
    </WaNoChoice>

    <WaNoDictBox name="Charge and multiplicity">
        <WaNoInt name="Charge">0</WaNoInt>
        <WaNoChoice name="Multiplicity">
        <Entry id="0" chosen="True" >singlet</Entry>
        <Entry id="1">doublet</Entry>
        <Entry id="2">triplet</Entry>
        </WaNoChoice>
    </WaNoDictBox>

  </WaNoDictBox>

  <WaNoDictBox name="Analysis">
    <WaNoDictBox name="Partial charges">
     <WaNoBool name="Mulliken charges">false</WaNoBool>
     <WaNoBool name="ESP fit">false</WaNoBool>
    </WaNoDictBox>
  </WaNoDictBox>


 </WaNoRoot>

 <WaNoExecCommand>bash ./exec.script
 </WaNoExecCommand>

 <WaNoInputFiles>
   <WaNoInputFile logical_filename="exec.script">exec.script</WaNoInputFile>
   <WaNoInputFile logical_filename="nwchem.in">input_template.dat</WaNoInputFile>
   <WaNoInputFile logical_filename="analyse.py">analyse_template.py</WaNoInputFile>
   <WaNoInputFile logical_filename="report.py">report_template.py</WaNoInputFile>
 </WaNoInputFiles>

 <WaNoOutputFiles>
    <WaNoOutputFile>nwchem.out</WaNoOutputFile>
 </WaNoOutputFiles>

</WaNoTemplate>



