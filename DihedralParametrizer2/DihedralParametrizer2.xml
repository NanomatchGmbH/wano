<WaNoTemplate>
 <WaNoRoot name="DihedralParametrizer2">
  <WaNoFile logical_filename="molecule.pdb" name="Molecule pdb">pdb file here</WaNoFile>
  <WaNoFile logical_filename="molecule.spf" name="Molecule spf">spf file here</WaNoFile>
  <WaNoInt name="Number of Steps">18</WaNoInt>
  <WaNoDropDown name="relax engine">
    <Entry id="0">FF</Entry>
    <Entry id="1">DFTB_noscc</Entry>
    <Entry id="2">DFTB</Entry>
  </WaNoDropDown>
  <WaNoDropDown name="evaluation engine">
    <Entry id="0">DFTB</Entry>
    <Entry id="1">DFT</Entry>
    <Entry id="2">DFTB_noscc</Entry>
  </WaNoDropDown>
  <WaNoInt name="scf iterations">2</WaNoInt>
  <WaNoFloat name="MC step multiplier">1.0</WaNoFloat>
  <WaNoInt name="evaluation points">100</WaNoInt>
  <WaNoDictBox  visibility_condition="'%s' == 'DFT'" visibility_var_path="evaluation engine" name="DFT Parameters">
    <WaNoDropDown name="Basis">
      <Entry id="0" chosen="True" >SV(P)</Entry>
      <Entry id="1">def2-SV(P)</Entry>
      <Entry id="2">SVP</Entry>
      <Entry id="3">def2-SVP</Entry>
      <Entry id="4">TZVP</Entry>
      <Entry id="5">def2-TZVP</Entry>
      <Entry id="6">TZVPP</Entry>
      <Entry id="7">def2-TZVPP</Entry>
    </WaNoDropDown>
    <WaNoDropDown name="Functional">
      <Entry id="0" chosen="True" >b-p</Entry>
      <Entry id="1">b3-lyp</Entry>
    </WaNoDropDown>
  </WaNoDictBox>


 </WaNoRoot>
 <WaNoExecCommand>bash ./dihedral_parametrizer.sh </WaNoExecCommand>
 <WaNoInputFiles>
   <WaNoInputFile logical_filename="dihedral_parametrizer.sh">dihedral_parametrizer.sh</WaNoInputFile>
 </WaNoInputFiles>
 <WaNoOutputFiles>
    <WaNoOutputFile>molecule.pdb</WaNoOutputFile>
    <WaNoOutputFile>dihedral_forcefield.spf</WaNoOutputFile>
 </WaNoOutputFiles>
</WaNoTemplate>
