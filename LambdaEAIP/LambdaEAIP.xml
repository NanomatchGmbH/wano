<WaNoTemplate>
    <WaNoRoot name="LambdaEAIP">
        <WaNoDictBox name="Molecule">
            <WaNoFile logical_filename="molecule.xyz" name="Path">molecule.xyz</WaNoFile>
            <WaNoInt name="Charge">0</WaNoInt>
            <WaNoInt name="Multiplicity">0</WaNoInt>
        </WaNoDictBox>
        <WaNoDictBox name="General Settings">
            <WaNoBool name="Calculate Lambda">False</WaNoBool>
            <WaNoDropDown name="Lambda Type" visibility_condition="%s == True" visibility_var_path="General Settings.Calculate Lambda">
                <Entry id="0" chosen="True">Both</Entry>
                <Entry id="1">Hole only</Entry>
                <Entry id="2">Electron only</Entry>
            </WaNoDropDown>
            <WaNoBool name="Calculate EA">False</WaNoBool>
            <WaNoBool name="Calculate IP">False</WaNoBool>
            <WaNoBool name="Vertical EA/IP">True</WaNoBool>
        </WaNoDictBox>
        <WaNoDictBox name="Geometry Optimization Options">
            <WaNoDropDown name="Basis">
                <Entry id="0" chosen="True" >SV(P)</Entry>
                <Entry id="1">def2-SV(P)</Entry>
                <Entry id="2">SVP</Entry>
                <Entry id="3">def2-SVP</Entry>
                <Entry id="4">TZVP</Entry>
                <Entry id="5">def2-TZVP</Entry>
                <Entry id="6">TZVPP</Entry>
                <Entry id="7">def2-TZVPP</Entry>
            </WaNoDropDown>
            <WaNoDropDown name="Functional">
                <Entry id="0" chosen="True" >b-p</Entry>
                <Entry id="1">b3-lyp</Entry>
                <Entry id="2">pbe</Entry>
                <Entry id="3">tpss</Entry>
            </WaNoDropDown>
            <WaNoInt name="Memory">1500</WaNoInt>
            <WaNoInt name="Threads">1</WaNoInt>
        </WaNoDictBox>
        <WaNoDictBox name="Single Point Options">
            <WaNoDropDown name="Basis">
                <Entry id="0" chosen="True" >SV(P)</Entry>
                <Entry id="1">def2-SV(P)</Entry>
                <Entry id="2">SVP</Entry>
                <Entry id="3">def2-SVP</Entry>
                <Entry id="4">TZVP</Entry>
                <Entry id="5">def2-TZVP</Entry>
                <Entry id="6">TZVPP</Entry>
                <Entry id="7">def2-TZVPP</Entry>
            </WaNoDropDown>
            <WaNoDropDown name="Functional">
                <Entry id="0" chosen="True" >b-p</Entry>
                <Entry id="1">b2-plyp</Entry>
                <Entry id="2">b3-lyp</Entry>
                <Entry id="3">pbe</Entry>
                <Entry id="4">tpss</Entry>
            </WaNoDropDown>
            <WaNoInt name="Memory">1500</WaNoInt>
            <WaNoInt name="Threads">1</WaNoInt>
       </WaNoDictBox>
    </WaNoRoot>
<WaNoExecCommand>export NANOMATCH=/home/nanodev_build/nanomatch; source $NANOMATCH/configs/quantumpatch.config; python3 lambda_ea_ip.py</WaNoExecCommand>
<WaNoInputFiles>
    <WaNoInputFile logical_filename="lambda_ea_ip.py">lambda_ea_ip.py</WaNoInputFile>
</WaNoInputFiles>
<WaNoOutputFiles>
     <WaNoOutputFile>report.zip</WaNoOutputFile>
     <WaNoOutputFile>opt_charge_0.xyz</WaNoOutputFile>
     <WaNoOutputFile>opt_charge_+1.xyz</WaNoOutputFile>
     <WaNoOutputFile>opt_charge_-1.xyz</WaNoOutputFile>
     <WaNoOutputFile>detailed.yml</WaNoOutputFile>
</WaNoOutputFiles>
</WaNoTemplate>
