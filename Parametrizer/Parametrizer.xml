<WaNoTemplate>
 <WaNoRoot name="Parametrizer">
  <WaNoFile logical_filename="molecule.mol2" name="Molecule (Mol2)">Mol2 input file</WaNoFile>
  <WaNoDictBox name="Geometrical Optimization">
      <WaNoBool name="Optimize">False</WaNoBool>
      <WaNoChoice name="Basis">
        <Entry id="0" chosen="True" >SV(P)</Entry>
        <Entry id="1">def2-SV(P)</Entry>
        <Entry id="2">SVP</Entry>
        <Entry id="3">def2-SVP</Entry>
        <Entry id="4">TZVP</Entry>
        <Entry id="5">def2-TZVP</Entry>
        <Entry id="6">TZVPP</Entry>
        <Entry id="7">def2-TZVPP</Entry>
      </WaNoChoice>
     <WaNoChoice name="Functional">
        <Entry id="0" chosen="True" >b-p</Entry>
        <Entry id="1">b3-lyp</Entry>
      </WaNoChoice>
  </WaNoDictBox>
  <WaNoDictBox name="Partial Charge Settings">
      <WaNoChoice name="Basis">
        <Entry id="0" chosen="True" >SV(P)</Entry>
        <Entry id="1">def2-SV(P)</Entry>
        <Entry id="2">SVP</Entry>
        <Entry id="3">def2-SVP</Entry>
        <Entry id="4">TZVP</Entry>
        <Entry id="5">def2-TZVP</Entry>
        <Entry id="6">TZVPP</Entry>
        <Entry id="7">def2-TZVPP</Entry>
      </WaNoChoice>
     <WaNoChoice name="Functional">
        <Entry id="0" chosen="True" >b-p</Entry>
        <Entry id="1">b3-lyp</Entry>
      </WaNoChoice>
     <WaNoChoice name="Partial Charge Method">
        <Entry id="0" chosen="True" >ESP</Entry>
        <Entry id="1">Mulliken</Entry>
      </WaNoChoice>
  </WaNoDictBox>
  <WaNoDictBox name="Hardware Parameters">
    <WaNoChoice name="Engine">
    <Entry id="0" chosen="True" >Turbomole</Entry>
    <Entry id="1">Gaussian</Entry>
    </WaNoChoice>
    <WaNoFloat name="DFT Memory [MB]">1000</WaNoFloat>
  </WaNoDictBox>
 </WaNoRoot>
 <WaNoExecCommand>bash ./param_intro.sh</WaNoExecCommand>
 <WaNoInputFiles>
   <WaNoInputFile logical_filename="param_intro.sh">param_intro.sh</WaNoInputFile>
 </WaNoInputFiles>
 <WaNoOutputFiles>
    <WaNoOutputFile>report.zip</WaNoOutputFile>
    <WaNoOutputFile>report.odt</WaNoOutputFile>
    <WaNoOutputFile>molecule.spf</WaNoOutputFile>
    <WaNoOutputFile>molecule.pdb</WaNoOutputFile>
    <WaNoOutputFile>molecule.mol2</WaNoOutputFile>
 </WaNoOutputFiles>
</WaNoTemplate>
